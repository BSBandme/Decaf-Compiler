class foo {
	public static int p; 
	int fib(int n) {
		if(n <= 2)
			return 1;
		return fib(n - 1) + fib(n - 2);
	}
	public static void main() {
		foo.p = 2;
		int i, j;
		i = 0;
		while(i < 10) 
			i  = i + 1;
		for(i = 0; i < 10; i++) {
			j = j + i;
		}
		j = 3;
		if(i < 3) {
			j = j * 2;
		} else {
			j = j / 2;
		}
		i = fib(10);
	}
}

